<?xml version="1.0" encoding="utf-8" ?>

<Game>

  <E_20>
    <Print>
      Test, No Attributes//BREAK// maybe it does
    </Print>
    
    <Print EI ="21">
      Test, Attribute 21
    </Print>

    <Branch>
      <Option Tag ="A">
      
        <Input Tag ="Click">
        One
        </Input>

        <Roll>
          <SetCondition>
            isEIEnabled(21) + Player.Age > 2
            Player.Age = 1
          </SetCondition>
          <Set_1>
            Player.Strength +2 > D(7)
            Player.Age > Player.Strength
          </Set_1>
          S_1
        </Roll>

        <Result>
          <Set_1>
            Player.Age == 13
            Link(21)
          </Set_1>
          S_1
        </Result>
      
      </Option>

      <Option Tag ="B">

        <Input Tag ="Type">
          Two
        </Input>

        <Roll>
          <SetCondition>
            Player.Age > 2
            Player.Age = 1
          </SetCondition>
          <Set_1>
            Player.Strength + 5 > D(7)
            Player.Age > Player.Strength
          </Set_1>
          S_1
        </Roll>

        <Result>
          <Set_1>
            Player.Age == 13
            Link(21)
          </Set_1>
          S_1
        </Result>

      </Option>
    </Branch>
    
  </E_20>
  <E_21>
    <Print>
      This is set 21
    </Print>

    <Print EI ="20">
      Your comming from set 20
    </Print>
    
    
  </E_21>
  
</Game>