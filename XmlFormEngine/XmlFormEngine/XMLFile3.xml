<?xml version="1.0" encoding="utf-8" ?>

<Game>

  <E_20>
    <Print>
      Test, No Attributes
    </Print>
    
    <Print EI ="21">
      Test, Attribute 21
    </Print>

    <Branch>
      <Option Tag ="A">
      
        <Input Tag ="Click">
        Clickme
        </Input>

        <Roll>
          <SetCondition>
            Player.Age > 2
            Player.Age = 1
          </SetCondition>
          <Set_1>
            Player.Strength + 5 > Dice
            Player.Age > Player.Strength
          </Set_1>
          S_1
        </Roll>

        <Result>
          <Set_1>
            Player.Age == 13
            Link(13)
          </Set_1>
          S_1
        </Result>
      
      </Option>
    </Branch>
    
  </E_20>
</Game>